meta {
  name: Create Account
  type: http
  seq: 1
}

post {
  url: {{baseUrl}}/signup
  body: json
  auth: none
}

body:json {
  {
    "email": "{{email}}",
    "password": "{{password}}"
  }
}

vars:pre-request {
  email: example@email.com
  password: Password123
}

assert {
  res.status: eq 204
}

script:post-response {
  bru.setNextRequest("Log in Account")
}
